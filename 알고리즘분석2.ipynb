      "source": [
        "n = int(input())\n",
        "i = 0\n",
        "li = []\n",
        "for i in range(0, n):\n",
        " a = list(map(int,input().split()))\n",
        " li.append(a)\n",
        "\n",
        "i = 0\n",
        "j = 0\n",
        "row = 0\n",
        "\n",
        "while (1):\n",
        "  if (i == n) or (j == n):\n",
        "    break\n",
        "  \n",
        "  if (li[i][j] == 0):\n",
        "    i +=1\n",
        "  else:\n",
        "    row = i\n",
        "    j +=1\n",
        "\n",
        "\n",
        "print(row)"
      ],
